generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  sessionID String   @unique
  socketID  String?
  name      String?
  univ      String?
}

model CatchmindRoom {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  painterId Int?
  answererList String?
  solution  String?
}
