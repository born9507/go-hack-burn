      <div class="title" id="board-title">SCORE BOARD</div>
      <div id="Seoul-uni" style="display: flex; justify-content: center; align-items: center; color: white;font-size: 20px; margin-top: 15px;">SCORE :<span id="score" style="display: flex; justify-content: center; align-items: center; font-size: 20px;">1</span></div>
      
    </div>
  </div>
  <div class="game__box">
    <div class="box" id="box">
      <canvas class="canvas1" id="canvas1"></canvas>
    </div>
    </div>
  </div>
