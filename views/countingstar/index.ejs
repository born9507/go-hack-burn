<body onload="draw();">
<div class="wrapper">
  <div class="info-container">
    <a href="/" class="nes-badge" id="nes-badge">
      <span class="is-warning">BACK</span>
    </a>    
    <i class="nes-ash" id="nes-ash"></i>
    <div class="nes-container is-rounded" id="game-title">
